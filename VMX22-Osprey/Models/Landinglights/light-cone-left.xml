<?xml version="1.0"?><PropertyList><path>light-cone.ac</path><animation>    <type>blend</type>	<object-name>Cone-texture</object-name>	<value>0.3</value></animation><animation>    <name>landinglight_ALL</name>	<object-name>Cone</object-name>	<object-name>Cone-texture</object-name>	<type>rotate</type>	<!-- <property>instrumentation/doors/landinglightpos/position-norm</property> -->	<property>sim/multiplay/generic/float[15]</property>    <factor> 0.100 </factor>	<interpolation>      <entry><ind> 0.0 </ind><dep>    0 </dep></entry>	  <entry><ind> 1.0 </ind><dep>  -90 </dep></entry>    </interpolation>    <center>      <x-m>  0.115 </x-m>      <y-m>  0.000 </y-m>      <z-m>  0.000 </z-m>    </center>    <axis>      <x> 0 </x>      <y> 1 </y>      <z> 0 </z>    </axis></animation><effect>  <inherits-from>Aircraft/VMX22-Osprey/Models/Landinglights/light-cone-left</inherits-from>  <object-name>Cone</object-name></effect><!--  --><animation>    <type>select</type>    <object-name>Cone</object-name>	<object-name>Cone-texture</object-name>		<condition>		<and>            <!-- <property>sim/model/lights/landing-lights/state</property> -->			<property>sim/multiplay/generic/int[10]</property>			<equals>            <property>sim/model/lights/git-landing-lights/state</property>			<value>1</value>			</equals>		</and>        </condition></animation></PropertyList>