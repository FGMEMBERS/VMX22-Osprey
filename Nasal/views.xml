<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-35.0</chase-distance-m>

  <view n="100">
    <name>Co pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
        <x-offset-m archive="y"> -0.540 </x-offset-m> <!--right-->
        <y-offset-m archive="y"> -0.700 </y-offset-m> <!--up-->
        <z-offset-m archive="y"> -5.850 </z-offset-m> <!--back-->
      <pitch-offset-deg> -10.000 </pitch-offset-deg>
      <default-field-of-view-deg>55</default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  1.801 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 10.637 </z-offset-m> <!--Back-->
    </config>
  </view>
  
  <view n="102">
    <name>Flight Officer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
        <x-offset-m archive="y">  0.540 </x-offset-m> <!--right-->
        <y-offset-m archive="y"> -0.700 </y-offset-m> <!--up-->
        <z-offset-m archive="y"> -4.550 </z-offset-m> <!--back-->
      <pitch-offset-deg> -10.000 </pitch-offset-deg>
	  <heading-offset-deg> -90.000 </heading-offset-deg>
      <default-field-of-view-deg>55</default-field-of-view-deg>
    </config>
  </view>
  
  <view n="103">
    <name>Cargo Area</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
        <x-offset-m archive="y"> -0.540 </x-offset-m> <!--right-->
        <y-offset-m archive="y"> -0.700 </y-offset-m> <!--up-->
        <z-offset-m archive="y">  0.550 </z-offset-m> <!--back-->
      <pitch-offset-deg> -10.000 </pitch-offset-deg>
	  <heading-offset-deg> -160.000 </heading-offset-deg>
      <default-field-of-view-deg>55</default-field-of-view-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
        <x-offset-m archive="y">  0.540 </x-offset-m> <!--right-->
        <y-offset-m archive="y"> -0.700 </y-offset-m> <!--up-->
        <z-offset-m archive="y"> -5.850 </z-offset-m> <!--back-->
        <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg>60</default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double">0</target-z-offset-m>
    </config>
  </view>

</PropertyList>
