<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	
	<!-- Paratrooper Dumy - will be changed later to a lateer opening paratrooper model-->
	
	<submodel> 
	<name>parachutist</name>
		<model>Models/Geometry/parachuter.xml</model>
		<trigger>controls/jump-signal</trigger>
		<speed>0</speed>
		<repeat>true</repeat>
		<delay>1.5</delay>
		<count>24</count>
			<x-offset> -19.5 </x-offset>
			<y-offset>  0.0 </y-offset>
			<z-offset> -8.5 </z-offset>
			<yaw-offset>180.0</yaw-offset>
			<pitch-offset>0.0</pitch-offset>
			<heading-offset-deg> 0.000 </heading-offset-deg>
		<cd>1.2</cd>
		<eda>100</eda>
		<weight>200</weight>
		<wind>true</wind>
		<buoyancy>20</buoyancy>
		<life>600</life>
	</submodel>

</PropertyList>
