<?xml version="1.0"?><PropertyList><path>light-cone.ac</path><animation>    <type>blend</type>    <object-name>Cone</object-name>	<property>instrumentation/doors/landinglightstate/position-norm</property></animation><animation>    <type>blend</type>	<object-name>Cone-texture</object-name>	<property>instrumentation/doors/landinglightstate/position-norm</property>	<min>0.3</min>	<max>1.0</max></animation><animation>    <name>landinglight_ALL</name>	<object-name>Cone</object-name>	<object-name>Cone-texture</object-name>	<type>rotate</type>	<property>instrumentation/doors/landinglightpos/position-norm</property>    <factor> 0.100 </factor>	<interpolation>      <entry><ind> 0.0 </ind><dep>    0 </dep></entry>	  <entry><ind> 1.0 </ind><dep>  -90 </dep></entry>    </interpolation>    <center>      <x-m>  0.115 </x-m>      <y-m>  0.000 </y-m>      <z-m>  0.000 </z-m>    </center>    <axis>      <x> 0 </x>      <y> 1 </y>      <z> 0 </z>    </axis></animation><effect>  <inherits-from>Aircraft/VMX22-Osprey/Models/Landinglights/light-cone-right</inherits-from>  <object-name>Cone</object-name></effect><!--  --><animation>    <type>select</type>    <object-name>Cone</object-name>	<object-name>Cone-texture</object-name>	<condition>		<not-equals>		<property>instrumentation/doors/landinglightstate/position-norm</property>		<value>1.0</value>		</not-equals>	</condition></animation></PropertyList>