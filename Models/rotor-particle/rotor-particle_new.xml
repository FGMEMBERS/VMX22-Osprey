<?xml version="1.0"?><PropertyList><!-- ==                                     == --><!-- == ROTOR LEFT & RIGHT PARTICLE SYSTEMS == --><!-- ==                                     == -->  <!-- == particles - left rotor DIRT == -->	<animation>		<name>dirt_left</name>			<object-name>dirt_left_01</object-name>			<object-name>dirt_left_02</object-name>	</animation>			<animation>		<type>rotate</type>		<object-name>dirt_left</object-name>		<property>orientation/pitch-deg</property>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>rotate</type>		<object-name>dirt_left</object-name>		<property>orientation/roll-deg</property>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>translate</type>		<object-name>dirt_left</object-name>		<!-- <property>position/gear-agl-m</property> -->		<property>sim/multiplay/generic/float[10]</property>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>	<!-- left rotor DIRT - move particle system with engine tilt -->	<animation>		<type>translate</type>		<object-name>dirt_left</object-name>		<property>sim/model/v22/animation_tilt</property>		<factor>0.05</factor>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation><!-- left rotor DIRT - roate particles experimental -->	<animation>		<type>rotate</type>		<object-name>dirt_left_01</object-name>		<property>rotors/tail/blade[0]/position-deg</property>		<factor> 1 </factor>		<center>			<x-m>-1.5</x-m>			<y-m>-6.8</y-m>			<z-m>-2.9</z-m>    </center>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>	<!-- left rotor DIRT - move particle system up over wather 	<animation>		<type>translate</type>		<object-name>dirt_left</object-name>				<condition>					<not>						<property>sim/paused</property>					</not>					<not>						<property>sim/crashed</property>					</not>					<less-than>						<property>position/altitude-agl-ft</property>						<value>70</value>					</less-than>					<greater-than>						<property>rotors/tail/rpm</property>						<value>20</value>					</greater-than>		</condition>				<property>/environment/terrain</property>		<interpolation>			<entry><ind> 0.0 </ind><dep>  1.000 </dep></entry>			<entry><ind> 1.0 </ind><dep>  0.000 </dep></entry>			<entry><ind> 3.0 </ind><dep>  0.000 </dep></entry>			<entry><ind> 4.0 </ind><dep>  0.000 </dep></entry>		</interpolation>				<axis>			<x>0</x>			<y>0</y>			<z>1</z>		</axis>	</animation>	-->		<!-- NORMAL GROUND left rotor DIRT - particle system controled by engines-->	<particlesystem>		<name>dirt_left_01</name>		<texture>smoke_dirt.png</texture>		<emissive>false</emissive>		<lighting>false</lighting>		<offsets>			<x-m>-1.5</x-m>			<y-m>-6.8</y-m>			<z-m>-2.9</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>			<!--<property>sim/model/bo105/particles/enabled</property> -->			<not>				<property>sim/paused</property>			</not>			<not>				<property>sim/crashed</property>			</not>			<less-than>				<!-- <property>position/altitude-agl-ft</property> -->				<property>sim/multiplay/generic/float[10]</property>				<!-- <value>100</value> value is canged to meter -->				<value>30</value>			</less-than>			<greater-than>				<property>rotors/tail/rpm</property>				<value>7</value> switched of for test				<!-- <value>10000</value> -->			</greater-than>		</condition>		<shooter>			<theta-min-deg>0</theta-min-deg>			<theta-max-deg>360</theta-max-deg>			<phi-min-deg>-0.5</phi-min-deg>			<phi-max-deg>35.2</phi-max-deg>			<speed-mps>				<!-- <property>rotors/tail/rpm</property> -->				<property>sim/multiplay/generic/float[13]</property>				<factor>1</factor>				<interpolation>					<entry><ind>  0.0 </ind><dep> 200.00 </dep></entry>					<entry><ind>  2.0 </ind><dep> 200.00 </dep></entry>					<entry><ind> 25.0 </ind><dep> 600.00 </dep></entry>				</interpolation>				<spread>75</spread>			</speed-mps>			<rotation-speed>				<x-max-deg-sec>15</x-max-deg-sec>				<x-min-deg-sec>-15</x-min-deg-sec>				<y-max-deg-sec>15</y-max-deg-sec>				<y-min-deg-sec>-15</y-min-deg-sec>				<z-max-deg-sec>15</z-max-deg-sec>				<z-min-deg-sec>-15</z-min-deg-sec>			</rotation-speed>		</shooter>		<counter>			<particles-per-sec>				<property>position/altitude-agl-ft</property> 				<property>sim/multiplay/generic/float[10]</property>				<factor>-40</factor>				<offset-m>2000</offset-m>			</particles-per-sec>		</counter>		<align>billboard</align>		<particle>			<start>				<color>					<red>						<value>0.95</value>					</red>					<green>						<value>0.95</value>					</green>					<blue>						<value>0.95</value>					</blue>					<alpha>						<property>rotors/tail/rpm</property>						<factor>0.00004</factor>						<max>0.02</max>					</alpha>				</color>				<size>					<value>0.5</value>				</size>			</start>			<end>				<color>					<red>						<value>1</value>					</red>					<green>						<value>1</value>					</green>					<blue>						<value>1</value>					</blue>					<alpha>						<property>rotors/tail/rpm</property>						<factor>0.0002</factor>						<max>0.04</max>					</alpha>				</color>								<size>					<!-- <value>0.8</value> -->										<!-- amount of Dust / end size of particle controlled by collective -->					<!-- <property>sim/model/v22/rotor/left/collective</property> -->					<property>sim/multiplay/generic/float[13]</property>					<!-- <factor>0.2</factor>					<min>0.8</min>					<max>1.5</max>					 -->					<factor>1</factor>					<interpolation>						<entry><ind>  0.0 </ind><dep> 0.8 </dep></entry>						<entry><ind>  2.0 </ind><dep> 1.8 </dep></entry>						<entry><ind> 11.0 </ind><dep> 1.9 </dep></entry>						<entry><ind> 25.0 </ind><dep> 3.0 </dep></entry>					</interpolation>					<!-- <interpolation>						<entry><ind>  0.0 </ind><dep> 0.0 </dep></entry>						<entry><ind>  2.0 </ind><dep> 0.8 </dep></entry>						<entry><ind> 25.0 </ind><dep> 3.0 </dep></entry>					</interpolation> -->					<spread>0.4</spread>				</size>			</end>			<life-sec>				<!-- <value>2.27</value> -->				<property>sim/multiplay/generic/float[13]</property>				<factor>1</factor>				<interpolation>					<entry><ind>  0.0 </ind><dep> 1.00 </dep></entry>					<entry><ind>  2.0 </ind><dep> 1.30 </dep></entry>					<entry><ind> 11.0 </ind><dep> 2.27 </dep></entry>					<entry><ind> 25.0 </ind><dep> 2.27 </dep></entry>				</interpolation>			</life-sec>			<mass-kg>0.015</mass-kg>			<radius-m>0.045</radius-m>		</particle>		<program>			<fluid>air</fluid>			<gravity>false</gravity>			<wind>true</wind>		</program>	</particlesystem>			<!-- Low GROUND left rotor DIRT - particle system controled by engines-->		<particlesystem>		<name>dirt_left_02</name>		<texture>smoke_dirt.png</texture>		<emissive>false</emissive>		<lighting>false</lighting>		<offsets>			<x-m>-1.5</x-m>			<y-m>-6.8</y-m>			<z-m>-2.9</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>			<!--<property>sim/model/bo105/particles/enabled</property> -->			<not>				<property>sim/paused</property>			</not>			<not>				<property>sim/crashed</property>			</not>			<less-than>				<!-- <property>position/altitude-agl-ft</property> -->				<property>sim/multiplay/generic/float[10]</property>				<!-- <value>100</value> value is canged to meter -->				<value>30</value>			</less-than>			<greater-than>				<property>rotors/tail/rpm</property>				<value>7</value>				<!-- <value>10000</value> -->			</greater-than>		</condition>		<shooter>			<theta-min-deg>0</theta-min-deg>			<theta-max-deg>360</theta-max-deg>			<phi-min-deg>-0.5</phi-min-deg>			<phi-max-deg>3.2</phi-max-deg>			<speed-mps>				<!-- <property>rotors/tail/rpm</property> -->				<property>sim/multiplay/generic/float[13]</property>				<factor>1</factor>				<interpolation>					<entry><ind>  0.0 </ind><dep> 20.00 </dep></entry>					<entry><ind>  2.0 </ind><dep> 20.00 </dep></entry>					<entry><ind> 25.0 </ind><dep> 30.00 </dep></entry>				</interpolation>				<spread>50</spread>			</speed-mps>			<rotation-speed>				<x-max-deg-sec>15</x-max-deg-sec>				<x-min-deg-sec>-15</x-min-deg-sec>				<y-max-deg-sec>15</y-max-deg-sec>				<y-min-deg-sec>-15</y-min-deg-sec>				<z-max-deg-sec>15</z-max-deg-sec>				<z-min-deg-sec>-15</z-min-deg-sec>			</rotation-speed>		</shooter>		<counter>			<particles-per-sec>				<property>position/altitude-agl-ft</property> 				<property>sim/multiplay/generic/float[10]</property>				<factor>-40</factor>				<offset-m>500</offset-m>			</particles-per-sec>		</counter>		<align>billboard</align>		<particle>			<start>				<color>					<red>						<value>0.95</value>					</red>					<green>						<value>0.95</value>					</green>					<blue>						<value>0.95</value>					</blue>					<alpha>						<property>rotors/tail/rpm</property>						<factor>0.00004</factor>						<max>0.02</max>					</alpha>				</color>				<size>					<value>0.5</value>				</size>			</start>			<end>				<color>					<red>						<value>1</value>					</red>					<green>						<value>1</value>					</green>					<blue>						<value>1</value>					</blue>					<alpha>						<property>rotors/tail/rpm</property>						<factor>0.0002</factor>						<max>0.04</max>					</alpha>				</color>								<size>					<!-- <value>0.8</value> -->										<!-- amount of Dust / end size of particle controlled by collective -->					<!-- <property>sim/model/v22/rotor/left/collective</property> -->					<property>sim/multiplay/generic/float[13]</property>					<!-- <factor>0.2</factor>					<min>0.8</min>					<max>1.5</max>					 -->					<factor>1</factor>					<interpolation>						<entry><ind>  0.0 </ind><dep> 0.8 </dep></entry>						<entry><ind>  2.0 </ind><dep> 1.8 </dep></entry>						<entry><ind> 11.0 </ind><dep> 5.1 </dep></entry>						<entry><ind> 25.0 </ind><dep> 7.2 </dep></entry>					</interpolation>					<!-- <interpolation>						<entry><ind>  0.0 </ind><dep> 0.0 </dep></entry>						<entry><ind>  2.0 </ind><dep> 0.8 </dep></entry>						<entry><ind> 25.0 </ind><dep> 3.0 </dep></entry>					</interpolation> -->					<spread>0.4</spread>				</size>			</end>			<life-sec>				<!-- <value>2.27</value> -->				<property>sim/multiplay/generic/float[13]</property>				<factor>1</factor>				<interpolation>					<entry><ind>  0.0 </ind><dep> 1.00 </dep></entry>					<entry><ind>  2.0 </ind><dep> 1.40 </dep></entry>					<entry><ind> 11.0 </ind><dep> 2.57 </dep></entry>					<entry><ind> 25.0 </ind><dep> 3.27 </dep></entry>				</interpolation>			</life-sec>			<mass-kg>0.035</mass-kg>			<radius-m>0.045</radius-m>		</particle>		<program>			<fluid>air</fluid>			<gravity>false</gravity>			<wind>true</wind>		</program>	</particlesystem>			<!-- OVER WATHER ONLY - left rotor DIRT - particle system controled by engines-->		<!-- OVER WATHER ONLY - left rotor DIRT - particle system controled by engines-->		<!-- OVER WATHER ONLY - left rotor DIRT - particle system controled by engines	<particlesystem>		<name>dirt_left_02</name>		<texture>dirt.rgb</texture>		<emissive>false</emissive>		<lighting>false</lighting>		<offsets>			<x-m>-1.5</x-m>			<y-m>-6.8</y-m>			<z-m>-2.1</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>						<not>				<property>sim/paused</property>			</not>			<not>				<property>sim/crashed</property>			</not>			<less-than>				<property>position/altitude-agl-ft</property>				<value>100</value>			</less-than>			<greater-than>				<property>rotors/tail/rpm</property>				<value>20</value>			</greater-than>				<equals>				<property>environment/